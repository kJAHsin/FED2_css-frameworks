{
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "strict",
  "parser": "typescript",
  "printWidth": 75,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "tabWidth": 4,
  "useTabs": false,
  "endOfLine": "lf",
  "trailingComma": "all",
  "plugins": ["prettier-plugin-tailwindcss"]
}
